{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "effect": {
            "type": "number",
            "minimum": 0,
            "maximum": 0
        },
        "staticColor": {
            "type": "object",
            "properties": {
                "hue": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 255
                },
                "sat": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 255
                },
                "val": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 255
                }
            },
            "required": [
                "hue"
            ]
        }
    },
    "required": [
        "effect"
    ]
}